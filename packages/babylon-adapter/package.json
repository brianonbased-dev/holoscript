{
  "name": "@holoscript/babylon-adapter",
  "version": "1.0.0",
  "description": "Babylon.js rendering adapter for HoloScript",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "holoscript",
    "babylonjs",
    "babylon.js",
    "3d",
    "vr",
    "xr",
    "webgl",
    "webgpu"
  ],
  "author": "HoloScript Team",
  "license": "MIT",
  "peerDependencies": {
    "@babylonjs/core": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "@babylonjs/loaders": {
      "optional": true
    }
  },
  "dependencies": {
    "@holoscript/core": "workspace:*"
  },
  "devDependencies": {
    "@babylonjs/core": "^6.40.0",
    "@babylonjs/loaders": "^6.40.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}

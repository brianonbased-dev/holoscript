{
  "name": "@holoscript/std",
  "version": "3.0.0",
  "description": "HoloScript Standard Library - Core types and utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./math": {
      "types": "./dist/math.d.ts",
      "import": "./dist/math.js"
    },
    "./collections": {
      "types": "./dist/collections.d.ts",
      "import": "./dist/collections.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "import": "./dist/string.js"
    },
    "./time": {
      "types": "./dist/time.d.ts",
      "import": "./dist/time.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "test": "vitest run"
  },
  "keywords": [
    "holoscript",
    "std",
    "standard-library",
    "3d",
    "spatial"
  ],
  "author": "Brian X Base Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brianonbased-dev/Holoscript.git",
    "directory": "packages/std"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}

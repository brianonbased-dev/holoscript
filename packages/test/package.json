{
  "name": "@holoscript/test",
  "version": "1.0.0",
  "description": "Testing framework for HoloScript applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@holoscript/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@holoscript/core": ">=2.0.0"
  },
  "keywords": [
    "holoscript",
    "testing",
    "test",
    "vr",
    "ar"
  ],
  "author": "HoloScript Team",
  "license": "MIT"
}

{
  "name": "@holoscript/test",
  "version": "2.1.0",
  "description": "Testing framework for HoloScript applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@holoscript/core": "workspace:*"
  },
  "devDependencies": {
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "puppeteer": "^21.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@holoscript/core": ">=2.0.0"
  },
  "keywords": [
    "holoscript",
    "testing",
    "test",
    "vr",
    "ar"
  ],
  "author": "Brian X Base Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brianonbased-dev/Holoscript.git",
    "directory": "packages/test"
  }
}

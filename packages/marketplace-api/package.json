{
  "name": "@holoscript/marketplace-api",
  "version": "1.0.0",
  "description": "HoloScript Trait Marketplace API - publishing, discovery, and dependency resolution",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@holoscript/core": "workspace:*",
    "@holoscript/registry": "workspace:*",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^9.1.1",
    "semver": "^7.6.0",
    "zod": "^3.23.0",
    "winston": "^3.11.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/semver": "^7.5.6",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "tsx": "^4.7.0"
  },
  "keywords": [
    "holoscript",
    "marketplace",
    "traits",
    "registry",
    "api"
  ],
  "author": "HoloScript Team",
  "license": "MIT"

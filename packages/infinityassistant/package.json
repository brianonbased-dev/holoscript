{
  "name": "@holoscript/infinityassistant",
  "version": "2.0.2",
  "description": "Client for building universal apps with Infinity Builder from HoloScript",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "holoscript",
    "infinity-builder",
    "universal-apps",
    "cross-platform",
    "ai-building",
    "components",
    "api-client"
  ],
  "author": "HoloScript Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/holoscript/holoscript.git",
    "directory": "packages/infinityassistant"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1"
  }
}

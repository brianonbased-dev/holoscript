{
  "name": "@holoscript/runtime",
  "version": "2.1.0",
  "description": "HoloScript browser runtime - React Three Fiber integration, event bus, storage, and device APIs",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.js",
      "require": "./dist/events.cjs"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.js",
      "require": "./dist/storage.cjs"
    },
    "./device": {
      "types": "./dist/device.d.ts",
      "import": "./dist/device.js",
      "require": "./dist/device.cjs"
    },
    "./timing": {
      "types": "./dist/timing.d.ts",
      "import": "./dist/timing.js",
      "require": "./dist/timing.cjs"
    },
    "./math": {
      "types": "./dist/math.d.ts",
      "import": "./dist/math.js",
      "require": "./dist/math.cjs"
    },
    "./navigation": {
      "types": "./dist/navigation.d.ts",
      "import": "./dist/navigation.js",
      "require": "./dist/navigation.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    },
    "./global": {
      "import": "./dist/holoscript.global.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@holoscript/core": "workspace:*",
    "cannon-es": "^0.20.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "@react-three/fiber": "^8.0.0",
    "three": "^0.160.0",
    "monaco-editor": "^0.45.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@react-three/fiber": {
      "optional": true
    },
    "three": {
      "optional": true
    },
    "monaco-editor": {
      "optional": true
    }
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "@types/node": "^25.2.0",
    "@types/react": "^18.2.0",
    "@types/three": "^0.160.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "holoscript",
    "runtime",
    "react-three-fiber",
    "3d",
    "vr",
    "spatial-programming"
  ],
  "author": "Brian X Base Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brianonbased-dev/Holoscript.git",
    "directory": "packages/runtime"
  }
}

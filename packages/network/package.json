{
  "name": "@holoscript/network",
  "version": "1.0.0",
  "description": "Multiplayer networking for HoloScript (@networked trait)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "holoscript",
    "multiplayer",
    "networking",
    "webrtc",
    "websocket",
    "xr",
    "vr"
  ],
  "author": "HoloScript Team",
  "license": "MIT",
  "dependencies": {
    "@holoscript/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}

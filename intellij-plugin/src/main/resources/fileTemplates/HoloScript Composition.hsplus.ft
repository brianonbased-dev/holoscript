composition "${NAME}" {
  @manifest {
    title: "${NAME}"
    version: "1.0.0"
    author: "${USER}"
  }

  @world_metadata {
    theme: "default"
    mood: "neutral"
  }

  // Environment
  environment {
    @skybox { preset: "day" }
    @ambient_light { intensity: 0.5, color: "#FFFFFF" }
  }

  // Objects

}
